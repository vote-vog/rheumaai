{
  "cli": {
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false,
    "requireCommit": false
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "SUPABASE_URL": "$SUPABASE_URL",
          "SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "env": {
          "SUPABASE_URL": "$SUPABASE_URL", 
          "SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
